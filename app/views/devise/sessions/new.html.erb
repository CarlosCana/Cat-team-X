<!-- Preloader-->
<%= render 'shared/preloader' %>
<!-- Preloader end-->

<!-- Header-->
<%= render 'shared/header' %>
<!-- Header end-->

<!-- Wrapper-->
<div class="wrapper">
    <section id="global-background">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6 m-auto">
                    <div class="m-b-20">
                      <h2> Sign in </h2>
                        <h6>Sign into your account</h6>
                    </div>
                    <div class="m-b-20">

                          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                            <div class="form-group">
                                <%= f.email_field :email, placeholder:'Email', autofocus: true, autocomplete: "email", :class => 'form-control' %>
                            </div>
                            <div class="form-group">
                                <%= f.password_field :password, placeholder:'Password', autocomplete: "current-password", :class => 'form-control' %>
                            </div>
                            <div class="form-group">
                                <%= f.submit 'Log in', :class => 'btn btn-block btn-round btn-brand'%>
                            </div>
                            <% if devise_mapping.rememberable? -%>
                              <div class="field">
                                <%= f.check_box :remember_me %>
                                <%= f.label :remember_me %>
                              </div>
                            <% end -%>
                        <% end %>
                    </div>
                    <div class="m-b-20">
                        <p><small>Dont have an account yet? <a href="#">Create account</a> <br>
                                Forgot your username or password? <a href="#">Recover account</a></small></p>
                  </div>
                </div>
            </div>
        </div>
    </section>


    <!-- Footer-->
    <%= render 'shared/footer' %>
    <!-- Footer end-->
</div>
<!-- Wrapper end-->

